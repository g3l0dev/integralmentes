---
interface Props {
  title: string;
  body: string;
}
const { title, body } = Astro.props;
---

<details name="psycologi">
  <summary>{title}</summary>
  {body}
</details>

<style>
  details {
    border: solid 1px #181818;
    padding: 0.5rem;
    display: flex;
    flex-direction: column;
    gap: 2rem;
  }
</style>
